
enum TokenType {
	//Single char
	LEFT_PAREN, RIGHT_PAREN, LEFT_BRACE, RIGT_BRACE,
	COMMA, DOT, MINUS, PLUS, SEMICOLON, SLASH, STAR,
	
	//1 or 2 chars
	BANG, BAG_EQUAL,
	EQUAL, EQUAL_EQUAL,
	GREATER, GREATER_EQUAL,
	LESS, LESS_EQUAL,
	//Literals
	IDENTIFIER,
	STRING,
	NUMBER,
	//Keywords
	VAR, AND, CLASS, ELSE, FALSE, FUN, FOR, IF, NIL, OR, 
	PRINT, RETURN, SUPER, THIS, TRUE, WHILE,
	
	EOF
}
